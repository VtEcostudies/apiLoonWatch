{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "AddPermAtlasUser",
            "Effect": "Allow",
            "Principal": {
                "AWS": "arn:aws:iam::824614856275:user/vpatlas"
            },
            "Action": [
                "s3:GetObject",
                "s3:PutObject",
                "s3:DeleteObject"
            ],
            "Resource": "arn:aws:s3:::vpatlas.data/*"
        },
        {
            "Sid": "AddPermPublicUser",
            "Effect": "Allow",
            "Principal": "*",
            "Action": "s3:GetObject",
            "Resource": "arn:aws:s3:::vpatlas.data/*"
        },
        {
            "Sid": "AddPermAdminUser",
            "Effect": "Allow",
            "Principal": {
                "AWS": "arn:aws:iam::824614856275:user/jloomis"
            },
            "Action": "s3:*",
            "Resource": "arn:aws:s3:::vpatlas.data/*"
        }
    ]
}
